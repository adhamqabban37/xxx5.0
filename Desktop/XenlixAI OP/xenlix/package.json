{
  "name": "xenlix",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "playwright install",
    "format": "prettier --write .",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,html,mjs,cjs}\" \"scripts/**/*.{ts,tsx,js,mjs,cjs}\" \"tests/**/*.{ts,tsx,js}\"",
    "validate:env": "node validate-env.js",
    "test:cache": "node test-cache-performance.js",
    "smoke:crawl4ai": "tsx scripts/smoke/crawl4ai.smoke.ts",
    "smoke:hf": "tsx scripts/smoke/hf.smoke.ts",
    "test:hf": "tsx tests/hf.test.ts",
    "dev:crawl4ai:win": "powershell -ExecutionPolicy Bypass -File ./scripts/python_bootstrap.ps1 && powershell -ExecutionPolicy Bypass -Command \"cd crawl4ai_service; .\\.venv-crawl4ai\\Scripts\\activate; python run.py\"",
    "dev:crawl4ai:unix": "bash ./scripts/python_bootstrap.sh && (cd crawl4ai_service && source .venv-crawl4ai/bin/activate && python run.py)",
    "dev:web": "cross-env NODE_OPTIONS=--max-old-space-size=4096 next dev -p 3000",
    "dev:all:win": "concurrently -k -n CRAWL4AI,WEB -c blue,green \"npm:dev:crawl4ai:win\" \"wait-on http://localhost:8001/health && npm:dev:web\"",
    "dev:all:unix": "concurrently -k -n CRAWL4AI,WEB -c blue,green \"npm:dev:crawl4ai:unix\" \"wait-on http://localhost:8001/health && npm:dev:web\"",
    "kill:3000": "node -e \"require('child_process').exec(process.platform==='win32' ? 'for /f \\\"tokens=5\\\" %a in (\\\"netstat -ano ^| findstr :3000 ^| findstr LISTENING\\\") do taskkill /F /PID %a' : 'lsof -ti :3000 | xargs kill -9')\"",
    "kill:8001": "node -e \"require('child_process').exec(process.platform==='win32' ? 'for /f \\\"tokens=5\\\" %a in (\\\"netstat -ano ^| findstr :8001 ^| findstr LISTENING\\\") do taskkill /F /PID %a' : 'lsof -ti :8001 | xargs kill -9')\"",
    "smoke:web": "tsx scripts/smoke/web.smoke.ts",
    "smoke:all": "npm run smoke:crawl4ai && npm run smoke:web",
    "ai-vis:setup": "tsx scripts/ai-visibility-setup.ts",
    "ai-vis:queue-status": "tsx scripts/ai-visibility-queue-status.ts",
    "ai-vis:migrate": "prisma migrate dev --name add_ai_visibility_models",
    "ai-vis:migrate-prod": "prisma migrate deploy",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:brand-detection": "jest tests/enhanced-brand-detection.test.ts",
    "test:ai-visibility": "playwright test tests/ai-visibility.spec.ts",
    "test:api": "playwright test tests/api/*.spec.ts",
    "preaudit": "bash scripts/check-chrome.sh",
    "audit:lighthouse": "node scripts/run-lighthouse.js",
    "audit:ci": "npx lighthouse ${TARGET_URL:-http://localhost:3000} --chrome-flags='--headless=new --no-sandbox --disable-gpu --disable-dev-shm-usage' --output=json --output-path=./lighthouse-ci.json",
    "audit:docker": "docker-compose -f docker-compose.lighthouse.yml --profile audit up --build",
    "lighthouse:check": "bash scripts/check-chrome.sh",
    "lighthouse:version": "npx lighthouse --version",
    "lighthouse:setup": "bash scripts/setup-lighthouse.sh",
    "lighthouse:validate": "bash scripts/validate-lighthouse.sh",
    "lhci": "lhci autorun",
    "lhci:collect": "lhci collect",
    "lhci:assert": "lhci assert",
    "lhci:upload": "lhci upload",
    "test:vitest": "vitest",
    "test:vitest:run": "vitest run",
    "run:company-plugin": "tsx ./scripts/run-company-plugin.ts",
    "plugin-health": "node ./scripts/plugin-health.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@huggingface/inference": "^4.8.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.16.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
