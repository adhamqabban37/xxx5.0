    "@xenova/transformers": "^2.17.2",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.58.9",
    "cheerio": "^1.1.2",
    "chrome-launcher": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compromise": "^14.14.4",
    "firebase": "^12.3.0",
    "firebase-admin": "^13.5.0",
    "framer-motion": "^12.23.15",
    "googleapis": "^160.0.0",
    "ioredis": "^5.8.0",
    "jsdom": "^27.0.0",
    "leaflet": "^1.9.4",
    "lighthouse": "^12.8.2",
    "lucide-react": "^0.544.0",
    "mongodb": "^6.20.0",
    "mongoose": "^8.18.1",
    "nanoid": "^5.1.5",
    "natural": "^8.1.0",
    "next": "15.5.3",
    "next-auth": "^4.24.11",
    "pg": "^8.16.3",
    "playwright": "^1.55.0",
    "prisma": "^6.16.1",
    "puppeteer": "^24.22.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-hook-form": "^7.62.0",
    "react-leaflet": "^5.0.0",
    "react-pdf": "^9.2.1",
    "recharts": "^3.2.1",
    "redis": "^5.8.2",
    "resend": "^6.1.0",
    "robots-parser": "^3.0.1",
    "stripe": "^18.5.0",
    "undici": "^7.16.0",
    "zod": "^4.1.8",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/cli": "^4.1.14",
    "@tailwindcss/postcss": "^4",
    "@types/cheerio": "^1.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "jest": "^30.1.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwind-merge": "^3.3.1",
